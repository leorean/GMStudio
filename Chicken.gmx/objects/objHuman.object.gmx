            <string>if (!global.paused)
{
    timer = max(timer - 1,0);
    depth = D.LIVING + (room_height - y);
    
    switch (state)
    {
        case S.WAIT:
            xVel = 0;
            yVel = 0;
            if (timer == 0)
            {
                angle = random(360);
                timer = irandom(5)*20;
                state = S.WALK;
            }
        break;
        case S.WALK:
            xVel = lengthdir_x(spd,angle);
            yVel = lengthdir_y(spd,angle);
            if (timer == 0)
            {
                //angle = random(360);
                timer = irandom(3)*30;
                state = S.WAIT;
            }
        break;
        case S.PANIC:
            xVel = lengthdir_x(spdPanic*(stamina/maxStamina),angle);
            yVel = lengthdir_y(spdPanic*(stamina/maxStamina),angle);
            angle = (angle - 20 + random(40)) mod 360;
            
            stamina = max(stamina - choose(0,1,2),0);
            if (stamina == 0)
            {
                state = S.WALK;
                stamina = maxStamina;
            }
        break;
        case S.DIE:
            xVel *= .95;
            yVel *= .95;
            if (abs(xVel) == 0 &amp;&amp; abs(yVel) == 0)
            {
                instance_destroy();
            }
        break;
    }
    
    if (state != S.DIE)
    {
        //fall in panic
        source = instance_place(x+xVel,y+yVel,objChicken);
            if (source != noone)
                if (source.state != S.PANIC)
                    source = noone;
        if (source == noone)
        {
            source = instance_place(x+xVel,y+yVel,objHuman);
            if (source != noone)
                if (source.state != S.PANIC)
                    source = noone;
        }
        if (state != S.PANIC &amp;&amp; source != noone)
        {
            angle = (angle + 180) mod 360;
            state = S.PANIC;
        }
    }
        
    //collisions
    if (place_free(x+xVel,y) &amp;&amp; x+xVel &gt; 0 &amp;&amp; x+xVel &lt; room_width)
        x += xVel;
    else
    {
        xVel = 0;
        if (state != S.DIE)
            angle = (angle + 180) mod 360;
        timer = 0;
    }
    if (place_free(x,y+yVel) &amp;&amp; y+yVel &gt; 0 &amp;&amp; y+yVel &lt; room_height)
        y += yVel;
    else
    {
        yVel = 0;
        if (state != S.DIE)
            angle = (angle + 180) mod 360;
        timer = 0;        
    }

}
</string>
