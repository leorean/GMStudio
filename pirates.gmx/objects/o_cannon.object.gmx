            <string>if (state == 0) //rotation state
{
    angle += rot;
    
    if (angle &gt; maxangle || angle &lt; minangle)
        rot = -rot;
}

if (mouse_check_button_pressed(mb_left) &amp;&amp; state == 0)
{
    state = 1; //powerlevel state
    mouse_clear(mb_left);
}

if (state == 1)
{
    curpow += pow;
    if (curpow &gt; 1)
    {
        curpow = 1;
        state = 2;
    }
}

if (state == 2)
{
    curpow -= pow;
    if (curpow &lt; 0)
    {
        curpow = 0;
        state = 3; //missed your chance, playa.
    }
}


if (mouse_check_button_pressed(mb_left) &amp;&amp; in(state,1,2) || state == 3)
{
   with (o_player)
   {
        motion_set(o_cannon.angle, max(o_cannon.minpow, o_cannon.maxpow*o_cannon.curpow));
        state = SHOT;
        visible = true;
        o_cannon.state = 4;
   }
   
}


