        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (global.state &gt; STATE_PAUSED)
{

    if (global.state == STATE_GAMEOVER)
    {
        invincible = 0;
        state = WIN;
        xVel *= .9;
    }

    invincible = max(invincible-1,0);

    onGround = place_meeting(x,y+1,objGround)&amp;&amp;yVel&gt;=0;
    var proj = instance_place(x,y,objPlayerProjectile);
    
    if (hp &lt;= 0)
        state = DEAD;

    if (state == DEAD)
    {
        //death-impulse
        if (statePrev != DEAD)
        {
            spawnCoins(x,y,loot);
            deathTimer = 60;
        }
        
        deathTimer = max(deathTimer-1,0);
        
        if (!deathTimer)
            instance_destroy();
        
        if (onGround)
            xVel *= .95;
        
    }
        
    if (hp &gt; 0)
    {
        if (instance_exists(proj) &amp;&amp; !proj.dead)
        {
            proj.dead = true;
            chain = proj.chain;

            //hurt self
            var angle = point_direction(proj.x,proj.y,x,y);
            xVel = lengthdir_x(kb,angle);
            yVel = -kb;//lengthdir_y(kb,angle)-kb-1;
            hp = max(hp-global.dmg,0);
            invincible = maxInvincible;
            state = HURT;
            
            proj = noone;
        }
        
        if (x &gt; WIDTH)
        {
            if (instance_exists(objPlayer) &amp;&amp; !objPlayer.invincible)
            {
                global.hp = max(global.hp-1,0);
                objPlayer.invincible = objPlayer.maxInvincible;
                
                global.shake = true;
            }
            
            instance_destroy();
        }
    }
    
    if (state == IDLE)
    {
        switch(pattern)
        {
            case P_WALK:
                if (onGround)
                    xVel = sign(dir)* min(abs(xVel)+.05,spd);
            break;
        }
    }
    
    if (state == HURT || (state == DEAD &amp;&amp; !onGround))
    {
        if (max(abs(xVel),abs(yVel))&gt;1 &amp;&amp; chain&gt;0)
        {
            var e = instance_place(x,y,objEnemy);
            if (instance_exists(e) &amp;&amp; !e.invincible &amp;&amp; e.state != DEAD)
            {
                //hurt others
                
                var angle = point_direction(x,y,e.x,e.y);
                var pow = .9*max(abs(xVel),abs(yVel));
                
                e.xVel = lengthdir_x(pow,angle);
                e.yVel = lengthdir_y(pow,angle);
                e.hp = max(e.hp-floor(global.dmg*.3),0);
                e.invincible = e.maxInvincible;
                e.state = HURT;

                e.chain = max(chain-1,0);
                chain = 0;
                

            }
        }
        if (onGround)
            xVel *= .95;

        if (!invincible)
        {
            state = IDLE;
        }
    }
    
    //++++moving++++
    yVel += yGrav;
    if (!place_meeting(x+xVel,y,objGround))    
        x += xVel;
    else
        xVel = 0;
    if (!place_meeting(x,y+yVel,objGround))
    {
        y += yVel;
    }
    else
        yVel = 0;
        
    //++++end variables++++
    statePrev = state;
}
</string>
