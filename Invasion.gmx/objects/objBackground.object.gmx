daytime = (daytime + .01) % 100;

/*
(morning,midday,afternoon,evening,night)
r:244,166,75,234,67,74
g:194,232,218,108,46,106
b:164,244,235,185,94,138

(0,244) (1,166) (2,75) (3,234) (4,67) (5, 74) (6, 244)
(0,194) (1,232) (2,218) (3,108) (4,46) (5, 106) (6, 194)
(0,164) (1,244) (2,235) (3,185) (4,94) (5, 138) (6, 164)

*/

d = daytime/100 * 6;
    
r = 5.03788 * power(d,4) - 57.6768 * power(d,3) + 211.659 * power(d,2)-283.074 * d + 251.695;
g = -1.09848 * power(d,4) + 21.4596 * power(d,3) - 114.78 * power(d,2) + 154.822 * d + 189.589;
b = -0.950758 * power(d,4) + 18.2702 * power(d,3) - 105.027 * power(d,2) + 178.911 *d + 161.444;
//h = 0.0004 * power(daytime,2) + 0.32 * daytime - 8.3;
color = make_color_rgb(r,g,b);

if (global.state &gt; STATE_PAUSED)
show_debug_message(string(daytime));

//bg sky
draw_sprite_part_ext(sprBackground,-1,WIDTH,0,WIDTH,HEIGHT,0,0,1,1,color,1);

if (global.state &gt; STATE_PAUSED)
{
    dx = (dx - .25) % WIDTH;
}
//bg moving clouds
draw_sprite_part_ext(sprBackground,-1,3*WIDTH,0,WIDTH,HEIGHT,dx,0,1,1,color,1);
draw_sprite_part_ext(sprBackground,-1,3*WIDTH,0,WIDTH,HEIGHT,dx+WIDTH,0,1,1,color,1);

//bg scenery
draw_sprite_part_ext(sprBackground,-1,2*WIDTH,0,WIDTH,HEIGHT,0,0,1,1,color,1);

//rails/tracks
switch(global.path)
{
    case p0:
    draw_sprite(track0,-1,0,0);
    //draw_set_color(c_black);
    //draw_path(p0,0,0,true);
    break;
}

//fg floor
draw_sprite_part(sprBackground,-1,0,192 * (global.timer % 30 &gt; 15),WIDTH,HEIGHT,0,0);

