            <kind>1</kind>
            <string>if (global.state &gt; STATE_PAUSED)
{

    if (!taken)
    {
        yVel += yGrav;
        if (in(x+xVel,0,WIDTH))
            x += xVel;
        else
        {
            x = clamp(x,0,WIDTH);
            xVel = -xVel/2;
        }
            
        if (!place_meeting(x,y+yVel,objGround))
            y += yVel;
        else
        {
            hitGround = true;
            yVel = -yVel/2;
            xVel *= .9;
        }
        
        aliveTimer = max(aliveTimer-1,0);
        
        if (hitGround
        &amp;&amp; in(mouse_x,x-global.magnet,x+global.magnet)
        &amp;&amp; in(mouse_y,y-global.magnet,y+global.magnet)
        &amp;&amp; mouse_check_button(mb_left)
        &amp;&amp; global.state == STATE_PLAY)
        {
            global.coins += value;
            taken = true;
        }
    }
    else
    {
        if (!instance_exists(objPlayer)) instance_destroy();
        
        xVel = (objPlayer.x - x)/20+objPlayer.xVel;
        yVel = (objPlayer.y - y)/20+objPlayer.yVel;        
        x += xVel;
        y += yVel;
        //if (in(x,WIDTH-TILE,WIDTH) &amp;&amp; in(y,0,TILE))
        if (in(x,objPlayer.x-TILE,objPlayer.x+TILE)
         &amp;&amp; in(y,objPlayer.y-TILE,objPlayer.y+TILE))
            instance_destroy();
    }
    
    
    if (bbox_right&lt;0 || bbox_left &gt; WIDTH || y &gt; HEIGHT || !aliveTimer)
        instance_destroy();
}
</string>
