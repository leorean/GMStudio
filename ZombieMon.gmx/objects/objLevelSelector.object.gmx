levelParams = objControl.levelParams;
levelCount = array_length_1d(levelParams);
destx = 0;

offx = room_width/2;
space = 96;

mouse_x_prev = mouse_x;
selected = 0;

for (i = 0; i &lt; levelCount; i++)
{
    lvl[i] = instance_create((x + i*space)/* - 2.5*TILE*/, 128, objLvl);
    lvl[i].l = i;
