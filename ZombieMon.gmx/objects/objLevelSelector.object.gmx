            <string>mask_index = sprParticle;

bgx = 0;
bgy = 0;

x = room_width/2;
y = room_height;

selectY = 6*TILE;

xVel = 0;
yVel = 0;

state = 0;
desty = room_height;

levelParams = objControl.levelParams;
levelCount = array_length_1d(levelParams);
destx = 0;

offx = room_width/2;
space = 96;

dragx = mouse_x;
selected = 0;

btnPlay = false;
btnShop = false;
tapTimer = 0;

alpha = -.5;
bgspeed = .5;

for (i = 0; i &lt; levelCount; i++)
{
    lvl[i] = instance_create((x + i*space)/* - 2.5*TILE*/, selectY, objLvl);
    lvl[i].l = i;
}
