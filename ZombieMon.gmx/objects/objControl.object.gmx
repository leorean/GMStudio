            draw_set_font(global.fntSmall);
            draw_set_halign(fa_center);
            draw_set_valign(fa_top);

            draw_text(TILE + 2*i*TILE, h - 2.5*TILE-2, "x"+string(global.items[i]));
            draw_set_font(global.fnt);
        }
