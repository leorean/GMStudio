    coinTimer = max(coinTimer - 1, 0);
    if (coinTimer)
        draw_text(6*TILE, 1 + TILE - (1 - (coinTimer/maxCoinTimer))*TILE, "+" + string(global.coinCount));
    if (lastCoinCount != global.coinCount)
        coinTimer = maxCoinTimer;        
    lastCoinCount = global.coinCount;

