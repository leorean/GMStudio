    draw_sprite_part_ext(sprHud, -1, 5*TILE, 16*TILE, 6*TILE, TILE+2, 0, 0, 1, 1, c_white, .6);
        
    draw_text_colour(6*TILE + 2, 1, "$"+string(global.coins) + " ", c_white, c_white, c_lime, c_lime, 1);
    draw_set_color(c_white);
    
