            <string>if (!playerSelection)
{
    handleButtons();
    
    if (btn[0].pressed)
    {
        playerSelection = true;
        pm = instance_create(0,0,objPlayerMenu);
        pm.parent = id;
        //if (global.mode == M.TUTORIALMODE)
        //    room_goto(TUTORIAL);
        //else
        //room_goto(MAIN);
        
    }
    
    if (btn[1].pressed)
        room_goto(PREMIUM);
    
    if (btn[2].pressed)
        room_goto(SHOP);
    
    /*
    if (instance_exists(pme))
        pme.visible = true;
    if (instance_exists(hme))
        hme.visible = true;
    */
    
    for(i=0;i&lt;=maxIndex;i++)
    {
        btn[i].enabled = true;
        btn[i].visible = true;
    }

    if (!os_is_network_connected())
    {
        btn[3].enabled = false;
        btn[4].enabled = false;
    }
    else
    {
        btn[3].enabled = true;    
        btn[4].enabled = true;
    }
    
    if (btn[3].pressed)//achievements
    {
        if(!achievement_login_status())
                achievement_login();
                
        if (achievement_available())
            achievement_show_achievements();
    }
    
    if (btn[4].pressed)//leaderboard
    {
        if (os_is_network_connected())
        {
            if(!achievement_login_status())
            {
                achievement_login();
            }
            else
            {
                achievement_show_leaderboards();
            }
        }
    }
    
    if (in(mouse_x,0,2*TILE) &amp;&amp; in(mouse_y,0,2*TILE) &amp;&amp; mouse_check_button_released(mb_left))//help
    {
        global.mode = M.TUTORIALMODE;
        room_goto(MAIN);                
    }
} else
{
    /*
    if (instance_exists(pme))
        pme.visible = false;
    if (instance_exists(hme))
        hme.visible = false;
    */
    for(i=0;i&lt;=maxIndex;i++)
    {
        btn[i].enabled = false;
        btn[i].visible = false;
    }

}

