        target = instance_nearest(x,y,objEnemy);
    } else
    {
        if (target.x &lt; WIDTH+TILE)
        {
            xVel += lengthdir_x(.2,point_direction(x,y,target.x,target.y));
            yVel += lengthdir_y(.2,point_direction(x,y,target.x,target.y));
        }
        
        var e = instance_place(x+xVel,y+yVel,objEnemy);
        if (instance_exists(e))
        {
            killEnemy(e);
            alive = false;
        }
    }
    if (timer == 0) alive = false;
    
    if (!alive)
    {
        var eff = instance_create(x-.5*TILE,y-.5*TILE,objEffectDust);
        eff.type = 5;
        
        instance_destroy();

    }
    
    xVel = sign(xVel)*min(abs(xVel),5);
    yVel = sign(yVel)*min(abs(yVel),5);
    x += xVel;
    y += yVel;

}


</string>
