    p_description = global.p_description[index];
    p_price = global.p_price[index];
    
    switch (p_id)
    {
        case "noads":
            p_type = "Durable";
            if (global.noads)
                p_purchased = true;
        break;        
        case "unlockupgrades":
            p_type = "Durable";
            if (global.unlockupgrades)
                p_purchased = true;
        break;
        case "coinpack1":
        case "coinpack2":
        case "coinpack3":
            p_type = "Consumable";
        break;
        case "player1":
        p_type = "Durable";
        if (!global.playerLocked[1])
            p_purchased = true;
    }
    //p_type = global.p_type[index];
}

