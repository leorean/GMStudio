{    
    draw_sprite_part_ext(spritePremium,-1,WIDTH+3*TILE,6*TILE,3*TILE,3*TILE,x+TILE/4+2,y-TILE/4+2,1,1,c_black,.5);
    draw_sprite_part(spritePremium,-1,WIDTH+3*TILE,6*TILE,3*TILE,3*TILE,x+TILE/4,y-TILE/4);
}
