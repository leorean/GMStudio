{
    case "coinpack1":col = 1;break;
    case "coinpack2":col = 2;break;
    case "coinpack3":col = 3;break;
    case "noads":col = 4;break;
    case "unlockupgrades":col = 5;break;
    case "player1":col = 6;break;
    default:
        col = 0;
    break;
}
draw_sprite_part(spritePremium,-1,WIDTH+col*3*TILE,0,3*TILE,3*TILE,x,y);

var c1,c2;
if (id == P.product)
{
    c1 = c_white;
    c2 = c_black;
} else
{
    c1 = c_white;
    c2 = make_color_hsv(23,59,61);
}

drawText(x+3*TILE,y+3*TILE,priceText(p_price),c1,c2,fntTiny);

