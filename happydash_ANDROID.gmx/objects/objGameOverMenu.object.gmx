draw_set_halign(fa_center);
draw_set_valign(fa_top);

//rainbow effect
draw_set_alpha(.7);
for (i = 0; i &lt; TILE; i+=1)
{
    draw_set_color(make_color_hsv((i div 2)*22,255,255));
    draw_line_width(-2*TILE,dy+i,dx+TILE-abs(TILE/2-i),dy+i,1);
}
