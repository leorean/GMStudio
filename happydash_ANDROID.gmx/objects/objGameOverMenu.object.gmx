        e = instance_create(dx-TILE+random(2*TILE),dy-TILE+random(2*TILE),objEffectDust);
        e.type = 3;
        e.depth = depth-1;
        e.xVel = -1+random(2);
        e.yVel = -1+random(2);
    }
    
} else
{
    if (global.timer mod 2 == 0)
    {
        c = instance_create(random(WIDTH),-TILE,objEffectConfetti);
        c.depth = depth-2;
    }
}

