{
    dx = min(dx+8,WIDTH+2*TILE);
    draw_sprite_part(spritePlayer,-1,(floor(global.timer/3) mod 4)*2*TILE,6*TILE,2*TILE,2*TILE,dx-TILE/2,dy-TILE/2);
