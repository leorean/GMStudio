        <arguments>
          <argument>
            <kind>1</kind>
            <string>var row = 0;
switch(state)
{
    case IDLE:
        row = 0;
        fSpeed = .1;
    break;
    case WALK:
        row = 1;
        fSpeed = .2 * kVel;
    break;
    case JUMP:
        row = 2;
        if (yVel &lt; 0)
            fCur = 0;
        else fCur = 2;
    break;
    case DEAD:
        fCur = 0;
        row = 3;
    break;
}
fCur = (fCur + fSpeed) % fMax;
frame = floor(fCur);
draw_sprite_part_ext(sprPlayer, -1, frame*32, row*32, 32, 32, x + 16*(dir==LEFT) - 16*(dir==RIGHT), y - 32, -1 + 2*(dir == RIGHT), 1, c_white, 1);
</string>
