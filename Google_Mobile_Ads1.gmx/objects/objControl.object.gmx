          <argument>
            <kind>1</kind>
            <string>///Draw our status info

// Interstitials
draw_text(objLoadInterstitial.x + 160, objLoadInterstitial.y + 10, string(GoogleMobileAds_InterstitialStatus()));

if (global.interLoading)
{
    if (GoogleMobileAds_InterstitialStatus() == "Ready") { global.interLoading = false; }
    else { draw_text(10,50, "Loading");    }
}

// Banners
draw_text(250,(room_height / 2), "Banner size = #"
+ string(GoogleMobileAds_BannerGetWidth()) + "x" +  string(GoogleMobileAds_BannerGetHeight()));


// Test ads
if (global.useTestAds) { draw_text(objToggleTest.x + 160, objToggleTest.y + 10, "Enabled"); }
else { draw_text(objToggleTest.x + 160, objToggleTest.y + 10, "Disabled"); }



</string>
