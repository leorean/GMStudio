        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Initialise

if (os_type == os_ios) 
{ 
    GoogleMobileAds_Init("ca-app-pub-1470453329682008/7720493774"); 
    global.bannerId = "ca-app-pub-1470453329682008/5459967373";
}
else 
{ 
    GoogleMobileAds_Init("ca-app-pub-1470453329682008/2849642170");
    global.bannerId = "ca-app-pub-1470453329682008/2570440579";
}

global.useTestAds = false;      // Note that before using the button for this you must have added your device ID into objToggleTest
global.bottomRight = false;
global.interLoading = false;
</string>
          </argument>
        </arguments>
