        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>cx = view_xview[0] + offx;
cy = view_yview[0] + offy;
x = cx;
y = cy;

global.kJump = btnJump.pressing;
global.kAttack = btnAttack.pressing;

for (var i = 0; i &lt; 5; i++)
if (in(device_mouse_x(i),bbox_left,bbox_right)&amp;&amp;in(device_mouse_y(i),bbox_top,bbox_bottom))
    if(device_mouse_check_button(i,mb_left))
    {
        dev = i;
    }
    
if (!device_mouse_check_button(dev,mb_left))
    dev = -1;

kLeft = false;
kRight = false;
kUp = false;
kDown = false;

    
if (dev&gt;-1)
{
    var angle = point_direction(cx,cy,device_mouse_x(dev),device_mouse_y(dev));
    var radius = w/2;
    var rad_angle = ((angle / 180) * pi);
        
    sx = cx + (cos(rad_angle)) * min(abs(cx-device_mouse_x(dev)),radius);
    sy = cy -(sin(rad_angle)) * min(abs(cy-device_mouse_y(dev)),radius);
    
    //send out arrow inputs according to angle
    if (in(angle,90,270))
        dir = LEFT;
    else
        dir = RIGHT;
    
    //if (in(angle,30,60) || in(angle,120,150) || in(angle,210,240) || in(angle,300,330))
    //    diag = true;
    if (in(angle,315,360) || in(angle,0,45))
        kRight = true;
        
    if (in(angle,45,135))
        kUp = true;
        
    if (in(angle,135,225))
        kLeft = true;
        
    if (in(angle,225,315))
        kDown = true;

} else //feather back
{
    sx -= (sx - cx) / 4;
    sy -= (sy - cy) / 4;
}

forceX = abs(cx - sx) / (w/2 - TILE);
forceY = abs(cy - sy) / (h/2 - TILE);
</string>
          </argument>
