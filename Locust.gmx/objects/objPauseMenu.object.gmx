            <string>// bg
draw_sprite_part(sprUI, -1, 512, 0, 384, 304, 320, 48);

draw_set_font(global.font1);
draw_set_halign(fa_center);
draw_set_valign(fa_center);
draw_text(WIDTH/2, 96, "-Paused-");

var mx = device_mouse_x_to_gui(0);
var my = device_mouse_y_to_gui(0);

// PLAY (click outside)
if (!in(mx, 320, 704) || !in(my, 48, 352))
{
    if (device_mouse_check_button_released(0, mb_left))
    {
        global.state = G_PLAY;
        instance_destroy();
    }
}

// PLAY
if (in(mx, 480, 480+64) &amp;&amp; in(my, 128, 128+80))
{
    if (device_mouse_check_button(0, mb_left))
        playPressing = true;
    if (device_mouse_check_button_released(0, mb_left))
    {
        global.state = G_PLAY;
        instance_destroy();
    }
} else
    playPressing = false;
draw_sprite_part(sprUI, -1, 512, 304 + 80 * playPressing, 64, 80, 480, 128);

// RESTART
if (in(mx, 352, 352+64) &amp;&amp; in(my, 192, 192+80))
{
    if (device_mouse_check_button(0, mb_left))
        restartPressing = true;
    if (device_mouse_check_button_released(0, mb_left))
    {
        room_restart();
    }
} else
    restartPressing = false;
draw_sprite_part(sprUI, -1, 576, 304 + 80 * restartPressing, 64, 80, 352, 192);

// HOME
if (in(mx, 480, 480+64) &amp;&amp; in(my, 256, 256+80))
{
    if (device_mouse_check_button(0, mb_left))
        homePressing = true;
    if (device_mouse_check_button_released(0, mb_left))
    {
        room_goto(MENU);
    }
} else
    homePressing = false;
draw_sprite_part(sprUI, -1, 768, 304 + 80 * homePressing, 64, 80, 480, 256);

// MUTE
if (in(mx, 608, 608+64) &amp;&amp; in(my, 192, 192+80))
{
    if (device_mouse_check_button(0, mb_left))
        mutePressing = true;
    if (device_mouse_check_button_released(0, mb_left))
    {
        global.mute = !global.mute;
        mutePressing = false;
    }
} else
    mutePressing = false;
draw_sprite_part(sprUI, -1, 640+64*global.mute, 304 + 80 * mutePressing, 64, 80, 608, 192);
</string>
