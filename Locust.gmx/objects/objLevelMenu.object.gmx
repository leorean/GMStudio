            <string>page = 0;
maxPage = ds_list_size(global.levels) div 10;

// fill menu
//L1, "01", "Do The Tutorial", 0
for (var i = 0; i &lt; ds_list_size(global.levels); i++)
{
    var level = ds_list_find_value(global.levels, i);
    item = instance_create(288 + (i % 5) * 96, 128 + ((i div 5) % 10) * 128, objLevelMenuItem);
    item.goto = level[0];
    item.name = level[1];
    item.description = level[2];
    item.stars = level[3];
    item.locked = i &gt; global.level;
    item.page = i div 10; // 10 levels per page        
}
</string>
