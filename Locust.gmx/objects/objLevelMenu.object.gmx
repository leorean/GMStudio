            <string>// menu border
draw_sprite_part(sprLevelMenu, -1, 0, 0, 512, 432, 256, 32);

draw_set_font(global.font1);
draw_set_halign(fa_center);
draw_set_valign(fa_top);
draw_text(WIDTH/2, 3*TILE, "Level Select");

// page thingy
for (var i = 0; i &lt; maxPage; i++)
{
    px = WIDTH/2 - 24*maxPage + i*24;
    draw_sprite_part(sprLevelMenu, -1, 96 + 16*(page == i), 496, 16, 16,px, 384);
}

//buttons
left = false;
right = false;
if (page &gt; 0)
{
    if (in(mouse_x, 128, 128+64) &amp;&amp; in(mouse_y, 432, 432+64))
    {
        if (mouse_check_button(mb_left)) left = true;
        if (mouse_check_button_pressed(mb_left)) page--;
    }
    draw_sprite_part(sprLevelMenu, -1, 128, 432 + 80 * left,64, 80, WIDTH/2 - 128, 418);
}
if (page &lt; maxPage)
{
    if (in(mouse_x, 192, 192+64) &amp;&amp; in(mouse_y, 432, 432+64))
    {
        if (mouse_check_button(mb_left)) right = true;
        if (mouse_check_button_pressed(mb_left)) page++;
    }
    draw_sprite_part(sprLevelMenu, -1, 192, 432 + 80 * right,64, 80, WIDTH/2 + 128, 418);
}
</string>
