        <arguments>
          <argument>
            <kind>1</kind>
            <string>// confetti stuff
confettiTimer = max(confettiTimer - 1, 0);
if (!confettiTimer)
{
    instance_create(0 + random(WIDTH) - 64, -64, objLevelMenuConfetti);
    confettiTimer = 20;
}

// funny potato stuff
ypotato = max(ypotato - 1, HEIGHT - 64);
draw_sprite_part(sprUI, -1, 256, 432, 96, 128, xpotato, ypotato);
if (ypotato == HEIGHT - 64)
    draw_sprite_part(sprUI, -1, 352, 432, 48, 48, xpotato + 80, ypotato - 32);
    
if (slideUp)
{
    var s = 16;
    
    y -= s;
    with (objLevelMenuItem) y -= s;
    
    if (y &lt;= -HEIGHT)
        room_goto(goto);
}

// menu border
draw_sprite_part(sprUI, -1, 0, 0, 512, 432, x + 256, y + 32);

draw_set_font(global.font1);
draw_set_halign(fa_center);
draw_set_valign(fa_top);
draw_text(x + WIDTH/2, y + 3*TILE, "Level Select");
    
// page thingy
for (var i = 0; i &lt;= maxPage; i++)
{
    px = WIDTH/2 - 8 - 12*(maxPage) + i*24;
    draw_sprite_part(sprUI, -1, 96 + 16*(page != i), 496, 16, 16, x + px, y + 384);
}

//buttons
left = false;
right = false;
if (page &gt; 0)
{
    if (in(mouse_x, x + 400, x + 400+64) &amp;&amp; in(mouse_y, y + 416, y + 416+64))
    {
        if (!slideUp)
        {
            if (mouse_check_button(mb_left)) left = true;
            if (mouse_check_button_released(mb_left)) page--;
        }
    }
    draw_sprite_part(sprUI, -1, 128, 432 + 80 * left,64, 80, x + 400, y + 416);
}

if (page &lt; maxPage)
{
    if (in(mouse_x, x + 560, x + 560+64) &amp;&amp; in(mouse_y, y + 416, y + 416+64))
    {
        if (!slideUp)
        {
            if (mouse_check_button(mb_left)) right = true;
            if (mouse_check_button_released(mb_left)) page++;
        }
    }
    draw_sprite_part(sprUI, -1, 192, 432 + 80 * right,64, 80, x + 560, y + 416);
}
</string>
