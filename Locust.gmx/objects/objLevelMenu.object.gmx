            <string>page = 0;
maxPage = ds_list_size(global.levels) div 10;
goto = MENU;
slideUp = false;

xpotato = 32;
ypotato = HEIGHT + 128;

instance_create(0, 0, objLevelMenuBackground);
confettiTimer = 0;

// fill menu
//L1, "01", "Do The Tutorial", 0
for (var i = 0; i &lt; ds_list_size(global.levels); i++)
{
    var level = ds_list_find_value(global.levels, i);
    item = instance_create(288 + (i % 5) * 96, 128 + ((i % 10) div 5) * 128, objLevelMenuItem);
    item.goto = level[0];
    item.name = level[1];
    item.description = level[2];
    item.stars = level[3];
    item.locked = i &gt; global.level;
    item.page = i div 10; // 10 levels per page        
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
