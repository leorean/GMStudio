    physics_apply_torque(-torque); 
}
if (kRight)
{
    if (phy_linear_velocity_x &gt; 20)
        dir = RIGHT;
    physics_apply_torque(torque);    
}

// reset jump
if (physics_test_overlap(x, y + 3, phy_rotation, objSolid) &amp;&amp; phy_linear_velocity_y &gt;= 0)
{
    jumped = false;
    if (state == JUMP) state = IDLE;
}

if (kJump)
{
    if (!jumped)
    {
        phy_linear_velocity_y = min(phy_linear_velocity_y, 0);
        physics_apply_local_force(0, 0, 0, -jumpVel);
        jumped = true;
        if (state == IDLE) state = JUMP;
    }
}

