        {
            dev = -1;
        }
    }
    else
    {
        kVel = 0;        
    }   
    if (device_mouse_check_button_pressed(i, mb_right))
        kJump = true;

    if (i &gt; 0 &amp;&amp; device_mouse_check_button_pressed(i, mb_left))
        kJump = true;
        
}

if (mouse_check_button_pressed(mb_right))
    kJump = true;

if (!kLeft) kLeft = keyboard_check(vk_left);
if (!kRight) kRight = keyboard_check(vk_right);
if (!kJump) kJump = keyboard_check_pressed(vk_up);
