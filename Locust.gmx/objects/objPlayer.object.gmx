kRight = false;
kJump = false;

for (i = 0; i &lt; 5; i ++)
{
    if (dev == -1)
    {
        if (device_mouse_check_button(i, mb_left))
            dev = i;
    }
    
    if (dev != -1)
    {        
        if (device_mouse_check_button(dev, mb_left))
        {
            kLeft = (device_mouse_x_to_gui(dev) &lt; view_wview *.3);
            kRight = (device_mouse_x_to_gui(dev) &gt; view_wview *.6)
        }
        
