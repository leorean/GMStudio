global.state = G_PLAY;
pauseScreen = noone;

global.mute = false; // &lt;- save/load

levelTextAlpha = 0;
global.levelText = "";
global.background = 0;

global.level = real(string_digits(room_get_name(room_last))) - 1; // &lt;- save/load (0, 1, 2, 3... = L1, L2, L3, L4...)

// ACCESSOIRES +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

// 50 lvls a la 3 stars = 150 stars in total
