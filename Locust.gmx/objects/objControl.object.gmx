global.colors[1] = array($FFFFFF, $FFFFFF, $7AD353, $6699BA, $C5F2FF, $DCF505, $73FFB2); // (forest) green/brown
global.colors[2] = array($FFFFFF, $FFFFFF, $5AF2B7, $84CFFA, $C5F2FF, $69FEFF, $FFFF40); // (desert) very green/lightbrown

//global.colors[0] = array($CFE6FF, $D9F7FF, $6BC6F7, $F2E2C4, $FEE2A4, $CA64F4, $FFFFA3); // (bonbon theme) yellow/grayblue with yellow sky
