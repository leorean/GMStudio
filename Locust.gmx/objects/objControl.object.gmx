if (keyboard_check_pressed(ord('Q')))
    game_restart();

if (room == MENU)
{
    if (keyboard_check_pressed(ord('R')) || keyboard_check_pressed(vk_backspace))
        room_goto_next();
}
else
{
    if (keyboard_check_pressed(ord('R')))
        room_restart();
}

if (global.state == G_PLAY)
{
    var w = view_wview;
    var h = view_hview;        
    instance_deactivate_object(objSolid);
    instance_activate_region(view_xview - w/2, view_yview - h/2, view_wview + w/2, view_hview + h/2, true);
    instance_deactivate_object(objBall); // OR ANY "LIVING"
    instance_activate_region(view_xview - w/3, view_yview - h/3, view_wview + w/3, view_hview + h/3, true);
    
    physics_pause_enable(false);
}

if (global.state == G_PAUSE)
{
    physics_pause_enable(true);
    instance_deactivate_all(true);
    instance_activate_object(objPauseMenu);
}
