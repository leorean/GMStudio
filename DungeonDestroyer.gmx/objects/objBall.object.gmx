        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (global.state != G_PLAY) exit;

if (hold)
{
    /*if (device_mouse_check_button(0, mb_left))
    {
        angle = point_direction(x, y, global.mx, global.my);
        hold = false;
    }
    else
    {
    }*/
    xVel = objBar.xVel;
    yVel = objBar.yVel;
    
} else
{

    /* collide with other balls */
    var c = collision_circle(x, y, w/2, objBall, false, true);
    if (instance_exists(c))
    {        
        c.angle = point_direction(x, y, c.x, c.y);
        angle = point_direction(c.x, c.y, x, y);
    }

    /* collide with blocks */
    var block_hor = collision_circle(x + xVel, y, w/2, objBlock, true, true);
    var block_ver = collision_circle(x, y + yVel, w/2, objBlock, true, true);
    
    if (block_hor)
    {
        if (block_hor.type != 1)
        block_hor.hp = max(block_hor.hp - 1, 0);
        
        if (!block_hor.falling &amp;&amp; block_hor.type != 0)
            angle = point_direction(0, 0, -xVel, yVel);
        //ballSpd += .1;
    }    
    if (block_ver)
    {
        if (block_ver.type != 1)
            block_ver.hp = max(block_ver.hp - 1, 0);

        if (!block_ver.falling &amp;&amp; block_ver.type != 0)
            angle = point_direction(0, 0, xVel, -yVel);
        //ballSpd += .1;
    }
    
    xVel = lengthdir_x(ballSpd, angle);
    yVel = lengthdir_y(ballSpd, angle);

}

if (y &gt; room_height + w)
{
    global.hp = max(global.hp - 1, 0);
    objBar.hurt = 1*SEC;
    instance_destroy();
}

if (in(x + xVel, BORDER + w/2, room_width - BORDER - w/2))
    x += xVel;
else
{
    angle = point_direction(x, y, x - xVel, y + yVel);
    //ballSpd += .1;
}

if (y + yVel &gt; BORDER + w/2 &amp;&amp; (y + yVel &lt; room_height - BORDER - w/2 || !global.invincible))
    y += yVel;
else
{
    angle = point_direction(x, y, x + xVel, y - yVel);
    //ballSpd += .1;
}

</string>
