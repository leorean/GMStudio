{
    _z = sin(global.timer * .1) * 2;
    draw_sprite_part(sprSlime, -1, 16*TILE + 3*TILE*(string_width(string(banner)) &gt; 19), 0,3*TILE,4*TILE, x - 1.5*TILE, y - 3.5*TILE + _z);
    
