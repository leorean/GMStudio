    _c = make_color_rgb(174, 140, 106);
    draw_set_halign(fa_center);
    draw_set_valign(fa_top);
    
    draw_text_color(x, y - 3 * TILE + _z, banner, _c, _c, _c, _c, 1);
    draw_set_font(global.fnt);
}

var alpha = .5 + !invisible * .5;

if (type == 13) f = floor(5 -  5*(hp / 10));// golem
else f = 0;

