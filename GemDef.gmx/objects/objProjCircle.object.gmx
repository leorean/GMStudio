        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (global.state == G_PAUSE) exit;

if (!instance_exists(parent) || (ds_list_size(enemiesKilled) &gt;= maxEnemies &amp;&amp; maxEnemies &gt; -1)
    || hp == 0)
{

    createEffect(x, y, 3);
    ds_list_destroy(enemiesKilled);
    instance_destroy();
}
else
{

    depth = 9 + room_height - y;

    r = (r + spd * global.spd) % 360;
    
    // diameter
    d = 4;
    if (type == 4) d = 12;
    
    if (hp &gt; 0)
        hp = max(hp - 1 * global.spd, 0);
    
    tx = parent.x + 8 + lengthdir_x(range, r);
    ty = parent.y + 8 + lengthdir_y(range, r);

    x += ((tx - x) / 8) * global.spd;
    y += ((ty - y) / 8) * global.spd;
        
    instance_deactivate_object(objSlime);
    instance_activate_region(x-d,y-d,2*d,2*d,true);

    for(i = 0; i &lt; instance_number(objSlime); i++)
    {        
        var s = instance_find(objSlime, i);
        if (s.initialized &amp;&amp; s.hp &gt; 0
            &amp;&amp; ds_list_find_index(enemiesKilled, s) == -1
            &amp;&amp; (ds_list_size(enemiesKilled) &lt; maxEnemies || maxEnemies == -1))
        {
            if ((type == 2 || type == 3) &amp;&amp; s.immuneToFire)
                continue;
            
            s.hp = max(s.hp - dmg, 0);
            if (maxEnemies != -1)
                ds_list_add(enemiesKilled, s);
        }
    }
    
    instance_activate_object(objSlime);    

}
</string>
