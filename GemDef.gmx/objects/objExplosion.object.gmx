{

    switch(type)
    {
        case 0:
            r = 1.5*TILE;
        break;
        case 1:
            r = 2*TILE;
        break;
        case 2:
            r = 2.5*TILE;
        break;
    }

    if (frame == 2 &amp;&amp; !exploded) // so it triggers only one time
    {
        exploded = true;
        instance_deactivate_object(objSlime);
        instance_activate_region(x-r,y-r,2*r,2*r,true);
        
        for(i = 0; i &lt; instance_number(objSlime); i++)
        {
            var s = instance_find(objSlime, i);
            
            if (instance_exists(s) &amp;&amp; s.initialized &amp;&amp; s.hp &gt; 0 &amp;&amp; distance_to_point(s.x, s.y) &lt;= r)
            {
                if (!s.immuneToFire)
                    s.hp = max(s.hp - dmg, 0);
                else
                    createEffect(s.x, s.y, 3);
            }
        }
        instance_activate_object(objSlime);
    }

    fCur = min(fCur + .35 * global.spd, 8);
    frame = floor(fCur);
    if (fCur == 8) instance_destroy();
}

draw_sprite_part(sprExplosion, -1, frame*4*TILE,type*4*TILE,4*TILE,4*TILE,x-2*TILE,y-2*TILE);
