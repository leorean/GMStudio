    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (mouse_x &gt; bbox_left &amp;&amp; mouse_x &lt; bbox_right
&amp;&amp; mouse_y &gt; bbox_top &amp;&amp; mouse_y &lt; bbox_bottom
&amp;&amp; mouse_check_button(mb_left))
{
    dragged = true;
}

if (mouse_check_button_released(mb_left))
    dragged = false;

if (dragged)
{
    phy_position_x = mouse_x;
    phy_position_y = mouse_y;
}

//++++++++++++++++++++++++++++++++++++++++
kLeft = keyboard_check(vk_left);
kRight = keyboard_check(vk_right);
kJump = keyboard_check(vk_up);
kAction = keyboard_check(vk_space);
kActionReleased = keyboard_check_released(vk_space);

if (!kAction)
{
    if (kLeft)
        dir = LEFT;
    if (kRight)
        dir = RIGHT;
}
if (abs(phy_speed_x) &lt; spdMax)
{
    if (kLeft)
        physics_apply_force(x,y,-spd,0);
    if (kRight)
        physics_apply_force(x,y,spd,0);
}

if (kActionReleased)
    canThrowRope = false;

if (kAction)
{
    if (canThrowRope &amp;&amp; !attached)
    {
        if (rope == noone)
        {
            rope = instance_create(x+sign(dir)*TILE/2,y-TILE/2,objRope);
            ropeOrig = rope;
            rope.parent = noone;
            rope.sticky = true;
            rope.orig = ropeOrig;
        }
        else
        {
            with(ropeOrig)
                physics_apply_impulse(x,y,sign(global.player.dir)*3,-2);

            if (ropeCount &lt; maxRopeCount)
            {
                physics_joint_delete(joint);
                r = instance_create(x+sign(dir)*TILE/2,y,objRope);
                r.parent = rope;
                r.orig = ropeOrig;
                
                //attach new rope segment  to its parent
                //attach = physics_joint_rope_create(r,rope,r.x,r.y,rope.x,rope.y,5,false);
                attach = physics_joint_revolute_create(r, rope, r.x, r.y, 0, 360, 0, 0, 0, 0, false); 

                rope = r;
                ropeCount += 1;
                
                joint = physics_joint_rope_create(self,rope,x,y,rope.x,rope.y,TILE/2,false);
            } else
                canThrowRope = false;
        }
    }
} else //GET ROPE BACK IN
{
    resetRope();
}

