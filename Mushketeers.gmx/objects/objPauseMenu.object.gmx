            <kind>1</kind>
            <string>if (!goingToRestart &amp;&amp; !goingToQuit)
{
    if (btnPlay.pressed || keyboard_check_pressed(vk_backspace))
    {
        global.state = G_PLAY;
    }
    
    if (btnRetry.pressed)
    {
        goingToRestart = true;
        global.curtain.state = CLOSE;
    }
       
    if (btnQuit.pressed)
    {
        goingToQuit = true;
        global.curtain.state = CLOSE;
    }
    
    if (global.mute)
    {
        btnSound.type = 4;
        btnSound.text = "sound off"
    }
    else
    {
        btnSound.type = 3;
        btnSound.text = "sound on"
    }
    
    if (btnSound.pressed)
    {
        global.mute = !global.mute;
        save();
    }
}
if (goingToRestart)
{
    if (global.curtain.isClosed)
        room_restart();
}
if (goingToQuit)
{
    if (global.curtain.isClosed)
        room_goto(WORLDMAP);
}

if (global.state != G_PAUSED)
    instance_destroy();



</string>
