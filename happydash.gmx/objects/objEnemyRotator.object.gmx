            <string>cycle ();

if (!global.pause)
{
    xO -= global.xSpeed;
    
    angle = (angle + spd) mod 360;
    rad_angle = ((angle / 180) * pi);
    for (i = 0; i &lt;= countSeg; i+=1)
    {
        r = radius/countSeg;
        tx[i] = (cos(rad_angle)) * r*i;
        ty[i] = -(sin(rad_angle)) * r*i;
        xx[i] = (xO+TILE/2) - tx[i];
        yy[i] = (yO+TILE/2) - ty[i];
        
    }
    x = (xO+TILE/2) - tx[countSeg];
    y = (yO+TILE/2) - ty[countSeg];

    if (!collision_point(xO+TILE/2,yO+TILE/2,objAny,false,true))
        alive = false;
        
    if (!alive)
    {
        for (i = 0; i &lt; countSeg; i+=1)
        {
            t = instance_create(xx[i],yy[i],objEffectDust);
            t.type = 2;
        }
        t = instance_create(x,y,objEnemy1);
        t.alive = false;
        
        playSound(sfxDestroyBlock,1);
        instance_destroy();
    }    
}



