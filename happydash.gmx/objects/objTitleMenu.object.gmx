          <argument>
            <kind>1</kind>
            <string>image_speed = 0;
draw_set_font(global.fntBig);
draw_set_color(c_white);
draw_set_alpha(1);
draw_set_halign(fa_center);
draw_set_valign(fa_top);

//hover title
t += .05;
if (t&gt;2*pi)
    t = 0;

z = 2*sin(t);

//background
xx = (xx - 1) mod WIDTH;

draw_sprite_part(spriteTitleMenu,-1,0,0,WIDTH,HEIGHT,xx,yy);
draw_sprite_part(spriteTitleMenu,-1,0,0,WIDTH,HEIGHT,xx+WIDTH,yy);
draw_sprite_part(spriteTitleMenu,-1,0,HEIGHT,WIDTH,HEIGHT,0,0);

if (!showHelp &amp;&amp; !showHighscore)
    drawBorder(7.5*TILE,6*TILE,10*TILE,8*TILE);
if (showHighscore)
    drawBorder(2*TILE,4*TILE,21*TILE,9*TILE);

//title
if (!showHelp &amp;&amp; !showHighscore)
{
    //draw_set_color(c_black);
    //draw_set_alpha(.3);
    //draw_sprite_part_ext(spriteTitleMenu,-1,0,2*HEIGHT,WIDTH,HEIGHT,0+4,-z+4,1,1,c_black,.5);
    draw_set_alpha(1);
    draw_set_color(c_white);
    draw_sprite_part(spriteTitleMenu,-1,0,2*HEIGHT,WIDTH,HEIGHT,0,-z);
}

draw_set_font(global.fntDefault);

if (showHelp || showHighscore)
{
    btn[0].visible = false;
    btn[1].visible = false;
    btn[2].visible = false;
    btn[3].visible = false;
} else
{
    btn[0].visible = true;
    btn[1].visible = true;
    btn[2].visible = true;
    btn[3].visible = true;
    
    textCreditsY = max(textCreditsY-.3,HEIGHT-TILE);
    draw_set_halign(fa_center);
    draw_set_valign(fa_top);
    draw_set_color(c_white);
    //draw_text(floor(WIDTH/2),floor(textCreditsY),textCredits);
    drawText(floor(WIDTH/2),floor(textCreditsY),textCredits,c_white,c_black,fntMenu);
    //drawText(WIDTH/2,textCreditsY,textCredits,c_white);
    /*draw_set_font(fntMenu);
    draw_set_color(c_black);
    draw_text(floor(WIDTH/2),floor(textCreditsY)+1,textCredits);
    draw_set_color(c_white);
    draw_text(floor(WIDTH/2),floor(textCreditsY),textCredits);*/
    draw_set_valign(fa_top);
    draw_set_font(global.fntDefault);
}


//HELP
if (showHelp)
{
    draw_set_halign(fa_center);
    draw_sprite_part(spriteTitleMenu,-1,WIDTH,page*HEIGHT,WIDTH,HEIGHT,0,0);
    drawText(floor(WIDTH/2),floor(TILE/2),"Help/Controls",c_white,c_black,fntButton);
}

//highscore
if (showHighscore)
{
    var suffix, row;
    row = 5*TILE;
    draw_set_halign(fa_center);
    //draw_text(WIDTH/2,row-z,"Highscores");
    drawText (WIDTH/2,row-z,"Highscores",c_white,c_black,fntButton);
    draw_set_font(global.fntDefault);
    for (i = 0; i &lt; global.scoreCount; i+=1)
    {
        if ((i+1) == 1)
            suffix = "&amp;&amp;&amp;&amp;&amp;";
        else if ((i+1) == 2)
            suffix = "&amp;&amp;&amp;&amp;";
        else if ((i+1) == 3)
            suffix = "&amp;&amp;&amp;";
        else if ((i+1) == 4)
            suffix = "&amp;&amp;";
        else if ((i+1) == 5)
            suffix = "&amp;";
        draw_set_halign(fa_left);        
        draw_text(3*TILE,row+1.5*TILE+i*TILE,string(suffix));
        draw_set_halign(fa_right);
        draw_text(WIDTH-3*TILE,row+1.5*TILE+i*TILE,string(global.scores[i]));
    }
}
