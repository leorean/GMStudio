            <string>if (!showHelp &amp;&amp; !showHighscore)
{
    handleButtons();
    objButton.visible = true;
} else
    objButton.visible = false;

screenPressed = mouse_check_button_released(mb_any) || keyboard_check_released(vk_anykey);

if (!showHighscore)
{
    if (btn[0].pressed)
        room_goto_next();
    if (btn[1].pressed)
        room_goto(SHOP);
    if (btn[2].pressed)
        showHighscore = true;
    if (btn[3].pressed)
        game_end();
} else if (screenPressed)
    showHighscore = false;

