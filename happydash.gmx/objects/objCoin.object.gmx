        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!global.pause)
{
    cycle();
    
    fCur = (fCur + fSpeed+2*fSpeed*!alive) mod fMax;
    frame = floor(fCur);

    if (alive)
    {

        if (spawn)
        {
            xVel = 1+random(2);//max(global.xSpeed,objPlayer.xVel);
            yVel = -2-random(1);
            yGrav = .15;
            spawn = false;
        }
        
        if (abs(global.player.x-x)&lt;TILE*.5 &amp;&amp; abs(global.player.y-y)&lt;TILE*.5) //optimized
            if (!alarm[0] &amp;&amp; magnet &amp;&amp; global.player.state != DEAD)
            {
                if (type == 0)
                {
                    global.coins += 1;
                    global.coinsCollected += 1;
                }
                else if (type == 1)
                {
                    global.coins += 100;
                    global.coinsCollected += 100;
                    t = instance_create(x+TILE*.5,y+TILE*.5,objText);
                    t.text = "$ + 100";

                }
                //each 5 segments, the coin is worth more!!
                global.addScore += (global.difficulty div 5);
                alive = false;
                playSound(sfxCoin,1);
            }
        
        if (!magnet)
        {   
            if (x &gt; WIDTH) xVel = 0;
            
            if (!alarm[0] &amp;&amp; !inWall)
            {
                if (place_meeting(x+xVel,y,objSolid))
                    xVel = 0;
                else
                    x += xVel;
            } else
                x += xVel;
    
            if (!alarm[0] &amp;&amp; !place_meeting(x,y,objSolid))
                inWall = false;
        
            yVel += yGrav;
            if (!alarm[0] &amp;&amp; !inWall)
            {
                if (place_meeting(x,y+yVel,objAny))
                {
                    if (yVel &gt;= 0) xVel = 0;
                        yVel = 0;
                } else
                    y += yVel;
            } else
                y += yVel;
                
        if (!alarm[0] &amp;&amp; !magnet &amp;&amp; in(global.player.x,x-global.distMagnet,x+global.distMagnet) &amp;&amp; in(global.player.y,y-global.distMagnet,y+global.distMagnet)) //optimized
            magnet = true;
            
        } else
        {
            x += global.xSpeed;
            x += xVel;
            y += yVel;
            xVel = ((global.player.x - x) * .33);
            yVel = ((global.player.y - y) * .33);
        }
        
        /*if (!alarm[0] &amp;&amp; alive &amp;&amp; place_meeting(x,y,objSolid))
            y -= 1;*/
    
    }
    else
    {
        xVel = 0;
        yVel = 0;
        y -= 1.5;
        alpha -= 1;
        if (alpha &lt;= 0)
            instance_destroy();
    }
} else
    alarm[0] += 1;

</string>
