        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!global.pause)
{
    if (x &lt; -TILE)
        instance_destroy();
    
    if (alive)
    {
        //x -= global.xSpeed;

        if (spawn)
        {
            xVel = 1+random(2);//max(global.xSpeed,objPlayer.xVel);
            yVel = -2-random(1);
            yGrav = .15;
            spawn = false;
        }
    
        if (!alarm[0] &amp;&amp; (place_meeting(x,y,objPlayer)) &amp;&amp; objPlayer.state != DEAD)        
        {
            global.coins += 1;
            //each n levels, the coin is worth more!!
            global.addScore += (global.theme+1);
            alive = false;
        }
    
        if (!magnet)
        {   
            if (!alarm[0] &amp;&amp; ((!inWall &amp;&amp; place_meeting(x+xVel,y,objSolid)) || x&gt;WIDTH))
            {
                xVel = 0;
            } else
                x += xVel;
    
            if (!alarm[0] &amp;&amp; !place_meeting(x,y,objSolid))
                inWall = false;
        
            yVel += yGrav;
            if (!alarm[0] &amp;&amp; !inWall &amp;&amp; place_meeting(x,y+yVel,objSolid))
            {
                if (yVel &gt;= 0) xVel = 0;
                yVel = 0;
            } else
                y += yVel;
        
        if (!alarm[0] &amp;&amp; collision_rectangle(x-global.distMagnet,y-global.distMagnet,x+2*global.distMagnet,y+2*global.distMagnet,objPlayer,true,true))
            magnet = true;

        } else
        {
            x += global.xSpeed;
            x += xVel;
            y += yVel;
            xVel = ((objPlayer.x - x) / 3);
            yVel = ((objPlayer.y - y) / 3);
        }
        
        /*if (!alarm[0] &amp;&amp; alive &amp;&amp; place_meeting(x,y,objSolid))
            y -= 1;*/
    
    }
    else
    {
        xVel = 0;
        yVel = 0;
        y -= 1.5;
        alpha -= .05;
        if (alpha &lt;= 0)
            instance_destroy();
    }
}
</string>
