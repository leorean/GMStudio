{
    instance_create(bbox_left+random(abs(bbox_left-bbox_right)),bbox_top+random(abs(bbox_top-bbox_bottom)),objParticlePoo);
    alarm[0] = alarm0;
}

if (hp &lt;= 0 || quadrant != global.quadrant || place_meeting(x,y,objEnemyBall))
    alive = false;

if (!place_meeting(x,y+2,objSolid) &amp;&amp; !place_meeting(x,y+2,objPlatform))
{
    xVel = 0;
}

if (!place_meeting(x+xVel,y,objSolid))
{
    x += xVel;
} else
{
    hp -= 1;
    dir = -dir;
    xVel = 0;
}

repeat(abs(yVel))
if (!place_meeting(x,y+sign(yVel),objSolid) &amp;&amp; !place_meeting(x,y+sign(yVel),objPlatform))
{
    y += sign(yVel);
} else
{
    yVel = 0;
}

if (!alive)
{
    repeat(50)
        instance_create(bbox_left+random(abs(bbox_left-bbox_right)),bbox_top+random(abs(bbox_top-bbox_bottom)),objParticlePoo);
    instance_destroy();
}
