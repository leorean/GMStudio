        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//touch/mouse
var b;
b = 3*TILE;

if (in(mouse_x,WIDTH/2-b,WIDTH/2+b) &amp;&amp; (global.input == INPUT_TOUCH || global.mouseVisible))
{

    for (i = 0; i &lt;= maxIndex; i+=1)
    {
        if (in(mouse_y,btn[i].y,btn[i].y+TILE))
            index = i;
    }
}

//keyboard
if(keyboard_check_pressed(vk_down))
{
    global.mouseVisible = false;
    index = min(index+1,maxIndex);
}

if(keyboard_check_pressed(vk_up))
{
    global.mouseVisible = false;
    index = max(index-1,0);
}

//unpause
if (keyboard_check_pressed(ord('P')) &amp;&amp; !alarm[0])
    global.pause = false;

//BUTTON ACTION
if (keyboard_check_pressed(vk_space) 
    || keyboard_check_pressed(vk_enter) 
    || mouse_check_button_pressed(mb_any))
{

    switch(index)
    {
        case 0: //unpause
            if ((global.input == INPUT_TOUCH || !global.mouseVisible) || in(mouse_y,btn[0].y,btn[0].y+TILE))
                global.pause = false;
        break;
        case 1: //restart
            if ((global.input == INPUT_TOUCH || !global.mouseVisible) || in(mouse_y,btn[1].y,btn[1].y+TILE))
                room_restart();
        break;
        case 2: //quit
            if ((global.input == INPUT_TOUCH || !global.mouseVisible) || in(mouse_y,btn[2].y,btn[2].y+TILE))
                room_goto(INIT);
        break;
    }
}

if (!global.pause || global.player.state == DEAD)
{
    with (objButton)
        instance_destroy();
    instance_destroy();
}

</string>
          </argument>
