        <arguments>
          <argument>
            <kind>1</kind>
            <string>z = 2*sin((global.timer *.03) mod 2*pi);

fCur = (fCur+fSpeed) mod (fMax+1);
frame = floor(fCur);
draw_sprite_part(spriteGfx,-1,2*frame*TILE,5*TILE,2*TILE,2*TILE,x-TILE,y-TILE+z);

draw_set_halign(fa_center);
draw_set_valign(fa_center);
draw_set_alpha(1);

drawText(WIDTH/2,HEIGHT/3,txt[i],c_white,c_black,fntButton);

if (!global.pause)
{
    //dont let the player DIE!!
    if (global.player.x &lt; TILE) global.player.x = TILE;
    
    //if (mouse_check_button_released(mb_left) || keyboard_check_released(vk_enter))
    //    timer = 0;

    timer = max(timer - 1, 0);    

    if (i &lt; 3)
        global.hasControl = false;
    else if (global.player.state != DEAD)
        global.hasControl = true;
        
    if (timer == 0)
    {
        if (i == 0)
            {i=1;timer = 3*FPS;exit}
        if (i == 1)
            {i=2;timer = 3*FPS;exit}
        if (i == 2)
        {
            for (j = 0; j &lt; 3*TILE; j+= TILE)
                instance_create(WIDTH+j,HEIGHT-4*TILE,objCoin);
            i=3;timer = 2*FPS;exit
        }
        if (i == 4)
        {
            for (j = 0; j &lt; 3*TILE; j+= TILE)
            {
                b = instance_create(WIDTH+2*j,HEIGHT-3*TILE,objDestroyBlock);
                b.type = 0;
            }
            i=5;timer = 2*FPS;exit
        }
        if (i == 6)
            {i=7;timer = 2*FPS;exit}
        if (i &gt;= 8 &amp;&amp; i &lt; 13)
            {i++;timer = 5*FPS;exit}
        if (i == 13)
        {
            global.mode = M.NORMALMODE;
            saveOptions();
            room_restart();
        }
    }
    
    with (objCoin) if (x &lt; TILE) x = WIDTH+TILE;
    with (objDestroyBlock) if (x &lt; 2*TILE) x = WIDTH+TILE;
    
    if (i == 3)
        if (global.player.state == JUMP &amp;&amp; !instance_exists(objCoin))
            {i=4;timer = 2*FPS;exit}
    if (i == 5)
        if (global.player.state == ATTACK &amp;&amp; !instance_exists(objDestroyBlock))
            {i=6;timer = 2*FPS;exit}
    if (i == 7)
        if (global.player.state == IDLE &amp;&amp; global.player.x &lt; WIDTH/3)
            {i=8;timer = 2*FPS;exit}
    
    
}
</string>
