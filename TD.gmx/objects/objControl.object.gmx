            <string>//++++debug etc++++

if (keyboard_check_pressed(vk_escape) || keyboard_check_pressed(vk_backspace))
    game_end();

if (keyboard_check(ord('T')))
    room_speed = 10;
else
    room_speed = 60;

if (keyboard_check_pressed(ord('S')))
    global.state = G_PLAY;
    
if (keyboard_check_pressed(ord('R')))
    room_restart();
if (keyboard_check_pressed(ord('K')))
    objPlayer.hp = 0;
    
if (keyboard_check_pressed(ord('P')))
    if (global.state == G_PLAY)
        global.state = G_PAUSED;
    else if (global.state == G_PAUSED)
        global.state = G_PLAY;

//++++
var cursor_x = (mouse_x div TILE) * TILE;
var cursor_y = (mouse_y div TILE) * TILE;

if (global.state &gt; G_PAUSED)
{
    global.timer = (global.timer+1) % INF;
}

if (mouse_check_button(mb_right))
{
    var tgt = collision_point(mouse_x,mouse_y,objPath,false,true);
    if (tgt != noone)
    {
        findPath(global.goal);
        with(tgt) instance_destroy();        
    }
}
if (mouse_check_button(mb_left) &amp;&amp; !instance_place(cursor_x,cursor_y, objPath))
{
    var newPth = instance_create(cursor_x,cursor_y,objPath);
    findPath(global.goal);

}
if (mouse_check_button_released(mb_left))
{
    //findPath(global.goal);
}
//++++enemy spawning++++

if (global.state == G_PLAY)
{
    if (delay == 0)
    {
        var r = floor(random(array_length_1d(global.spawn)));
        instance_create(global.spawn[r].x,global.spawn[r].y,objEnemy);
        delay = 2*SEC;
    } else
        delay = max(delay-1,0);
}
