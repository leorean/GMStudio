        if (dir == DOWN) blk = collision_point(cx,cy+TILE,objSolid,true,true);
        if (dir == LEFT) blk = collision_point(cx-TILE,cy,objSolid,true,true);
        if (dir == RIGHT) blk = collision_point(cx+TILE,cy,objSolid,true,true);
        
        if (instance_exists(blk))
        {
            with (blk) instance_destroy();
            hasBlock = true;
        }
    }
    
