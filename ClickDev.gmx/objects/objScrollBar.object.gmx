    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_set_alpha(1);
x = WIDTH-TILE;
yVel = 0;

if (global.ulMax &gt; 0)
{

    //++++drag &amp; change index++++
    if (!dragged) //only drag if not using the slider to the right
    {
        if (!global.scrolling)
        {
            dy = mouse_y;
        
            if (in(mouse_x,0,WIDTH-3*TILE)&amp;&amp;in(mouse_y,GROUND+3.5*TILE,HEIGHT)
                &amp;&amp; mouse_check_button(mb_left))
                global.scrolling = true;
        }
        
        if (mouse_check_button_released(mb_any))
            global.scrolling = false;
        
        if (global.scrolling)
        {
            if (mouse_y &gt; dy + 1.5*TILE)
            {
                global.u = max(global.u-1,0);
                with(objUpgrade)pressTimer = maxPressTimer;
                dy = mouse_y;
            }
            if (mouse_y &lt; dy - 1.5*TILE)
            {
                global.u = min(global.u+1,global.ulMax);
                with(objUpgrade)pressTimer = maxPressTimer;
                dy = mouse_y;
            }
        }
    }

    //++++scrollbar stuff++++
    
    draw_sprite_part(sprGfx,-1,WIDTH,2*TILE,TILE,3*TILE,x,y);
    
    if (in(mouse_x,x-TILE,x+TILE) &amp;&amp; mouse_y &gt; GROUND+3.5*TILE)// &amp;&amp; in(mouse_y,y,y+3*TILE))
    {
        if (mouse_check_button(mb_left))
        {
            dragged = true;
        } else
            dragged = false;
    } else
        if (!mouse_check_button(mb_left))
            dragged = false;
        
    if (dragged)
    {
        yVel = (mouse_y - (y+1.5*TILE))/5;
        y = clamp(y+yVel,GROUND+3.5*TILE,HEIGHT-3*TILE);
        global.u = floor(((y - (GROUND+3.5*TILE)) / ((HEIGHT-3*TILE)-(GROUND+3.5*TILE)))
            * global.ulMax);
            
        //show_debug_message("u:"+string(ind));
    }
    else
    {
        y = GROUND+3.5*TILE + 6*TILE*global.u/global.ulMax;
    }

} else
    y = GROUND+3.5*TILE;

//++++end++++
//y = clamp(y,GROUND+3.5*TILE, HEIGHT-3*TILE);

</string>
          </argument>
        </arguments>
      </action>
