    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//++++begin variables++++
if (statePrev != state) fCur = 0;

//++++state stuff++++

global.percent = round(100*((happiness+tidiness+hungriness+sleepiness)/4/100))/100;

if (state != PLAY)
{
    if (global.timer % (45*SEC) == 0)
        happiness = max(happiness - 15,0);
}

if (state == WALK)
{
    xVel = sign(dir)*.5;
    if (x + xVel &lt;= TILE || x + xVel &gt;= (WIDTH-TILE))
    {
        xVel = 0;
        dir = -dir;
    }
}

if (state == SLEEP)
{
    sleepiness = min(sleepiness + .01,100);
} else
{
    if (global.timer % (25*SEC) == 0)
        sleepiness = max(sleepiness - 10,0);
}

if (state != EAT)
    if (global.timer % (60*SEC) == 0)
        hungriness = max(hungriness-20,0);

tidiness = clamp(100-35*instance_number(objPoo),0,100);

if (!instance_exists(food))
{
    if (state == EAT) state = IDLE;
    if (hungriness &lt; 99 &amp;&amp; !place_meeting(x,y,objPoo))
        food = collision_line(x,y-TILE,x,y+TILE,objFood,false,true);
}
else
{
    if (state == WALK || state == IDLE)
    {
        state = EAT;
        stateTimer = .5*SEC;
    }
    if (state == EAT)
    {
        food.food = max(food.food-1,0);
        hungriness = min(hungriness+.2,100);
        
        var xp = .02*(global.level+1);
        global.xp += xp;
    }

}

if (state == YAWN)
{
    xVel = 0;
    //after animation, immediately go to state change
    if (fCur &gt;= fMax-fSpeed)
        stateTimer = 0;
    else
        stateTimer = 1*SEC;
}
var fingerCount = 0;
for (i = 0; i &lt; 5; i++)
    if (device_mouse_check_button(i,mb_any))
        fingerCount++;
        
for (i = 0; i &lt; fingerCount+1; i++)
{
    //if (collision_point(device_mouse_x(i),device_mouse_y(i),id,true,false)
    if (!place_meeting(x,y,objPoo) &amp;&amp; global.cursor == C_GRAB)
    {
        if (device_mouse_check_button(i,mb_left)
         &amp;&amp; (collision_point(device_mouse_x(i),device_mouse_y(i),id,true,false)))
        {
            food = noone;
            
            x = device_mouse_x(i);//+offx;
            y = device_mouse_y(i)-1*TILE;//+offy;
            xVel = 0;
            yVel = 0;
            
            //xVel = lengthdir_x(-min(4,abs(device_mouse_x(i)-x)),point_direction(device_mouse_x(i),device_mouse_y(i),x,y));
            //yVel = lengthdir_y(-min(4,abs(device_mouse_y(i)-y)),point_direction(device_mouse_x(i),device_mouse_y(i),x,y));
            
            if (state != HAPPY)
            {
                state = IDLE;
                stateTimer = 1*SEC;
            }
        }
    //if (in(device_mouse_y(i),7*TILE,14*TILE))
        if (device_mouse_check_button_pressed(i,mb_any) &amp;&amp; in(device_mouse_y(i),6.5*TILE,14*TILE))
        {
            state = HAPPY;
            
            var cpc = .5*global.cpc+.5*global.cpc*global.percent;
            
            global.xp += cpc;
            global.c += cpc;
            global.cmax += cpc;
                
            var txt = instance_create(x-2+random(4),y-2+random(4)-TILE,objText);
            txt.text = "+"+string(bigNumber(cpc))+"";
            
            stateTimer = 1*SEC;         
        }
    }
}

if (keyboard_check_pressed(ord('E')))
{
    state = EAT;
    stateTimer = 5*SEC;
}

//++++state transitions++++
poopTimer = max(poopTimer - 1,0);
stateTimer = max(stateTimer - 1, 0);

if (!stateTimer)
{
    stateTimer = choose(1,2,3)*SEC;
    
    //TODO: poop push notification
    if (!poopTimer &amp;&amp; onGround &amp;&amp; state == IDLE &amp;&amp; !place_meeting(x,y,objPoo))
    {
        instance_create(x,y+TILE,objPoo);
        poopTimer = maxPoopTimer;
    }
    
    switch(state)
    {
        case HAPPY:
            state = IDLE;         
        break;
        case IDLE:
            state = choose(WALK,IDLE,CLEAN);
            
            if (state == WALK)
                dir = choose(LEFT,RIGHT);
            
            //if (tidiness &lt; 50 &amp;&amp; min(hungriness,sleepiness,tidiness) == tidiness)
            //    state = choose(IDLE,CLEAN);
                
            if (state != WALK &amp;&amp; min(hungriness,sleepiness,tidiness) == sleepiness)
            {
                if (sleepiness &lt; 50)
                    state = choose(IDLE,YAWN);
                if (sleepiness &lt; 25)
                    state = YAWN;
                if (sleepiness &lt; 10)
                    state = SLEEP;
                    
            }
        break;
        case WALK:
            state = choose(WALK,IDLE,CLEAN);
        break;
        case SLEEP:
            if (sleepiness == 100)
                state = IDLE;
            /*
                state = choose(SLEEP,IDLE);
            else if (sleepiness &gt; 60 &amp;&amp; (hungriness &lt;= 30 || tidiness &lt;= 30))
                state = IDLE;
            */
        break;
        case CLEAN:
            state = choose(CLEAN,IDLE);
        break;
        case EAT:
            if (!instance_exists(food))
                state = IDLE;
        break;
        case YAWN:
            state = choose(IDLE,WALK,SLEEP);
            stateTimer = 2*SEC;
            //if (sleepiness &lt;= 20)
            //    state = choose(YAWN,IDLE);
        break;
        case PLAY:
            if (happiness &gt; 50)
                state = HAPPY;
            else
                state = IDLE;
        break;
        
    }
    if (state == YAWN) fCur = 0; //make sure the animation starts properly
}



//++++physics+++++

//onGround = place_meeting(x,y+1,objGround)&amp;&amp;yVel&gt;=0;
onGround = (y &gt;= GROUND &amp;&amp; yVel &gt;= 0);

yVel += yGrav;

//if (y +yVel &lt; 10.5*TILE)
//    yVel = -yVel/4;

y = clamp(y,7*TILE,GROUND);
if (!onGround)//place_meeting(x,y+yVel,objGround))
    y += yVel;
else
{
    yVel = 0;
    
    if(state != WALK)
        xVel *= .9;
   
}

x = clamp(x,TILE,WIDTH-TILE);
if (x + xVel &gt;= TILE &amp;&amp; x + xVel &lt;= WIDTH-TILE)
    x += xVel;
else
{
    x -= 3*sign(xVel);
    xVel = -xVel;
}
    
//++++end variables++++
statePrev = state;


</string>
          </argument>
        </arguments>
      </action>
