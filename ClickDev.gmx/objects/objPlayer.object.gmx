            <string>xVel = 0;
yVel = 0;
yGrav = .07;

offx = 0;
offy = 0;

dir = RIGHT;

state = IDLE;
stateTimer = 3*SEC;
statePrev = state;

happiness = 50;
tidiness = 50;
sleepiness = 50;
hungriness = 50;

food = noone;

w = abs(bbox_left-bbox_right);
h = abs(bbox_top-bbox_bottom);

fCur = 0;
fMax = 2;
frame = 0;

stateTimer = 0;
maxPoopTimer = 60*SEC;
poopTimer = maxPoopTimer;

onGround = false;
</string>
          </argument>
